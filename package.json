    "mongodb": "^3.5.5",
    "next": "9.3.4",
    "react": "16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-bootstrap-table-next": "^4.0.0",
    "react-dom": "16.13.1",
    "swr": "^0.2.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "heroku-dotenv": "^0.4.1",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1"
  }
